source :rubygems

# redmine_acts_as_taggable_on is a small wrapper around the Rails plugin
# acts-as-taggable-on (which itself provides the capability to add tags to
# records in the database). For more info, see:
#   https://github.com/hdgarrood/redmine_acts_as_taggable_on
gem 'redmine_acts_as_taggable_on', '~> 1.0'

